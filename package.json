{
  "name": "wedding-andrio-rut-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "test": "echo \"No Unit Test yet\"",
    "check:prettier": "prettier --config .prettierrc ./src/ --check",
    "format:prettier": "prettier --config .prettierrc ./src/ --write",
    "format:lint": "eslint --fix ./src/ --ext .js,.jsx,.ts,.tsx",
    "formatter": "yarn format:lint && yarn format:prettier"
  },
  "engines": {
    "node": ">=18",
    "yarn": "^1.22 <=2.*"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "3.2.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
